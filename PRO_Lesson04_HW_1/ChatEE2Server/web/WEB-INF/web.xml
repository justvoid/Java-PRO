<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://java.sun.com/xml/ns/javaee"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
    <servlet>
        <servlet-name>GetList</servlet-name>
        <servlet-class>net.ukr.just_void.GetListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetList</servlet-name>
        <url-pattern>/get</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>net.ukr.just_void.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Status</servlet-name>
        <servlet-class>net.ukr.just_void.StatusServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Status</servlet-name>
        <url-pattern>/status</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Add</servlet-name>
        <servlet-class>net.ukr.just_void.AddServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Add</servlet-name>
        <url-pattern>/add</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Group</servlet-name>
        <servlet-class>net.ukr.just_void.GroupServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Group</servlet-name>
        <url-pattern>/group</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
